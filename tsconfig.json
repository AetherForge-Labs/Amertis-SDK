{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "_version": "16.1.0",

  "compilerOptions": {
    "lib": ["es2021", "dom"],
    "module": "esnext",
    "target": "es2021",

    "strict": true,
    "esModuleInterop": true,
